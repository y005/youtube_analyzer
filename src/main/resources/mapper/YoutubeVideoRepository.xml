<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.project01.youtube.repository.YoutubeVideoRepository">
    <insert id="save" parameterType="com.example.project01.youtube.entity.YoutubeContent" >
        insert into youtube_content(user_id, refresh_token)
        values(#{param.user_id}, #{param.refresh_token});
    </insert>
</mapper>